<%= for {raw, item} <- @list do %>

  <div class="phx-hero card">
    Raw: <%= raw %>
    <br>
    <br>

    <%= for {k,v} <- item do %>

      <%= if k == "author" do %>

        <%= for author <- v do %>

          <%= if author["family"] do %>
            Author: <%= author["family"] %>
          <% end %>

        <% end %>
      <% end %>

    <% end %>
    <hr>
    <br>
    <br>
    <br>
  </div>
<% end %>
