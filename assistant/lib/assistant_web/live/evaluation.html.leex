<%= for {{raw, item}, {first_author, num_results}} <- @list do %>

  <div class="phx-hero card">
    <%= raw %>
    <br>
    <br>

    Anystyle Result:
    <br>
    <%= for {k,v} <- item do %>

      <%= if k == "author" do %>

        <%= for author <- v do %>

          <%= if author["family"] do %>
            Author: <%= author["family"] %>
          <% end %>

        <% end %>
      <% end %>

    <% end %>

    <br>
    <br>
    Zenon Result:
    <br>
    <%= if not is_nil(first_author) do %>
      Number results of zenon search for first author '<%= first_author %>': <%= num_results %>
    <% else %>
      None
    <% end %>

    <hr>
    <br>
    <br>
    <br>
  </div>
<% end %>
