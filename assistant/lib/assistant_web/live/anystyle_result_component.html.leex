<div style="text-align: left">
<%= for {k,v} <- AnystyleResultComponent.get_fields(@item) do %>
    <%= val = AnystyleResultComponent.convert(v)
        if val != "" do %>
        <div>
            <b>
                <%= AnystyleResultComponent.convert_key(k, @state.lang) %></b>: <%=
                val %>
        </div>
    <% end %>
<% end %>
</div>
