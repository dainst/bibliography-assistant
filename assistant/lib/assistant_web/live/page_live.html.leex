<%= if @current_page == "1" do %>
  <section class="main-card width" style="margin-top: 20px;">
    <%= live_component @socket,
      AssistantWeb.RawReferences,
      state: assigns,
      id: "raw-references" %>
  </section>
<% else %>
  <%= live_component @socket,
    AssistantWeb.Evaluation,
    id: "evaluation",
    state: assigns %>
<% end %>
